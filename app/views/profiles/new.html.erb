<h1>New Shizzle page</h1>
<p>

  <%= form_tag new_profile_path do %>
    <p>Email: <br>
    <%= email_field_tag :email %>
    </p>
    <p>UserName:<br>
    <%= text_field_tag :username%>
    </p>
    <p>Pin:<br>
    <%= text_field_tag :pin%>
    </p>
    <p>Website:<br>
    <%= text_field_tag :website%>
    </p>
    <p>Address:<br>
    <%= text_field_tag :address%>
    </p>
    <p>Alias:<br>
    <%= text_field_tag :alias%>
    </p>
    <%= submit_tag %>
  <% end %>

</p>



